{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "钓研社",
        "enablePullDownRefresh": false,
        "navigationBarBackgroundColor": "#667eea",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/spot/map",
      "style": {
        "navigationBarTitleText": "钓点地图",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/spot/create",
      "style": {
        "navigationBarTitleText": "发布钓点",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/weather/index",
      "style": {
        "navigationBarTitleText": "鱼情分析",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/social/index",
      "style": {
        "navigationBarTitleText": "社区交流",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/profile/index",
      "style": {
        "navigationBarTitleText": "个人中心",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/log/create",
      "style": {
        "navigationBarTitleText": "记录渔获",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "钓研社",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {},
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#667eea",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tab-home.png",
        "selectedIconPath": "static/tab-home-active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/spot/map",
        "iconPath": "static/tab-map.png",
        "selectedIconPath": "static/tab-map-active.png",
        "text": "钓点"
      },
      {
        "pagePath": "pages/social/index",
        "iconPath": "static/tab-community.png",
        "selectedIconPath": "static/tab-community-active.png",
        "text": "社区"
      },
      {
        "pagePath": "pages/profile/index",
        "iconPath": "static/tab-profile.png",
        "selectedIconPath": "static/tab-profile-active.png",
        "text": "我的"
      }
    ]
  },
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "首页",
        "path": "pages/index/index",
        "query": ""
      }
    ]
  }
}